public class LevelInfoBC : LevelInfo
{
    public mover.Class[] availableQuestions;
    public float speed;
    public float acceleration;
    public float spawnDelay;

    public LevelInfoBC(mover.Class[] aq, float s, float a, float d)
    {
        availableQuestions = aq;
        speed = s;
        acceleration = a;
        spawnDelay = d;
    }
}